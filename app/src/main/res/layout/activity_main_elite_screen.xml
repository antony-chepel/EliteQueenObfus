<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_elg"
    tools:context=".eltqwhite.MainEliteScreen">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="16sp"
        android:text="Attach to the right items for taking more treasures"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.060000002" />

    <Button
        android:id="@+id/b_el"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:text="Take treasure"
        android:backgroundTint="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.88" />

    <ImageView
        android:id="@+id/im_el_1"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="8dp"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.04000002"
        app:srcCompat="@drawable/elq_1" />

    <ImageView
        android:id="@+id/im_el_2"
        android:layout_width="120dp"
        android:elevation="10dp"
        android:layout_height="120dp"
        app:layout_constraintEnd_toEndOf="@+id/im_el_1"
        app:layout_constraintStart_toStartOf="@+id/im_el_1"
        app:layout_constraintTop_toBottomOf="@+id/im_el_1"
        app:srcCompat="@drawable/elq_2" />

    <ImageView
        android:id="@+id/im_el_3"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:elevation="10dp"
        app:layout_constraintEnd_toEndOf="@+id/im_el_2"
        app:layout_constraintStart_toStartOf="@+id/im_el_2"
        app:layout_constraintTop_toBottomOf="@+id/im_el_2"
        app:srcCompat="@drawable/elq_5" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/im_el_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/im_el_1"
        app:srcCompat="@drawable/elq_2" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/im_el_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/im_el_1"
        app:srcCompat="@drawable/elq_4" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="120dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="@+id/im_el_3"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/im_el_2"
        app:srcCompat="@drawable/elq_3" />
</androidx.constraintlayout.widget.ConstraintLayout>